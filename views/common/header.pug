header.header-area.header-sticky
  nav.main-nav.navbar.navbar-expand-md.navbar-light.bg-light.px-3
    a.navbar-brand(href="/")
      h1.h4.font-weight-normal Yoga Studio

    button.navbar-toggler(type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation")
      span.navbar-toggler-icon

    div.collapse.navbar-collapse#navbarNav
      ul.navbar-nav.mr-auto
        li.nav-item
          a.nav-link(href="/") Home
        li.nav-item
          a.nav-link(href="/classes") Classes
        li.nav-item
          a.nav-link(href="/instructors") Instructors
        li.nav-item
          a.nav-link(href="/contact") Contact Us
        li.nav-item
          a.nav-link(href="/admin") Admin

    div.search-input.ml-auto
      form#search.form-inline(action="#")
        input.form-control.mr-sm-2(type="text" placeholder="Type Something" id="searchText" name="searchKeyword" onkeypress="handle" disabled)

